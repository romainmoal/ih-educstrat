<style>
    min-width: 300px;
    max-width: 800px;
    height: 400px;
    margin: 1em auto;
    border: 1px solid silver;
    }

    #csv {
        display: none;
    }
</style>

<script src="https://code.highcharts.com/highcharts.js"></script>
<script src="https://code.highcharts.com/modules/sankey.js"></script>
<script src="https://code.highcharts.com/modules/exporting.js"></script>

<div id="container"></div>
{{#each ProfileModel}}
<p>{{this.schools.4.schoolName}} / {{this.schools.3.schoolName}} / {{this.schools.2.schoolName}} /
    {{this.schools.1.schoolName}} / {{this.schools.0.schoolName}}</p>
{{/each}}
<script>
    Highcharts.chart('container', {

        title: {
            text: 'Results'
        },
        series: [{
            keys: ['from', 'to', 'weight'],
            data: [
                ['Spain', 'England', 1],
                ['Brazil', 'Portugal', 1],
                ['Brazil', 'France', 1],
                ['Brazil', 'Spain', 1],
                ['Brazil', 'England', 1],
            ],
            type: 'sankey',
            name: 'Sankey demo series'
        }]

    });


</script>